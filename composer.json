{
    "name": "jmischer/cake-pdfreactor",
    "description": "CakePdf PdfReactor engine plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0",
        "friendsofcake/cakepdf": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "JMischer\\CakePDFreactor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JMischer\\CakePDFreactor\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
